{
    "lexicon": 1,
    "id": "app.bsky.actor.searchActors",
    "defs": {
      "main": {
        "type": "query",
        "description": "Get encrypted post body.",
        "parameters": {
          "type": "params",
          "properties": {
            "text": {
              "type": "string",
              "description": "Encrypted post body. It shoud be inlude text and additional with JSON format."
            }
          }
        },
        "output": {
          "encoding": "application/json",
          "schema": {
            "type": "object",
            "required": ["actors"],
            "properties": {
              "cursor": { "type": "string" },
              "actors": {
                "type": "array",
                "items": {
                  "type": "ref",
                  "ref": "app.bsky.actor.defs#profileView"
                }
              }
            }
          }
        }
      }
    }
  }
  