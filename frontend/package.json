{
  "name": "skyblur",
  "version": "0.4.0",
  "type": "module",
  "sideEffects": false,
  "private": true,
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@atcute/time-ms",
      "aws-sdk",
      "esbuild",
      "sharp",
      "unrs-resolver"
    ],
    "overrides": {
      "hono": "^4.11.7",
      "@modelcontextprotocol/sdk": "^1.26.0",
      "fast-xml-parser": "^5.3.4",
      "zod": "^4.3.6",
      "@aws-sdk/client-dynamodb": "3.972.0",
      "@aws-sdk/dynamodb-codec": "3.972.0"
    }
  },
  "scripts": {
    "dev": "next dev -p 4500",
    "update": "pnpm dlx npm-check-updates -u && pnpm install",
    "gen-lex": "lex-cli generate",
    "build": "next build",
    "start": "next start",
    "dev:aws": "pnpm dlx sst dev",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview --ip 192.168.1.130 --port 4500",
    "deploy:preview": "pnpm dlx sst deploy --stage preview",
    "deploy": "pnpm dlx sst deploy --stage production",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@atcute/atproto": "^3.1.10",
    "@atcute/bluesky": "^3.2.17",
    "@atcute/client": "^4.2.1",
    "@atcute/identity-resolver": "^1.2.2",
    "@atcute/identity-resolver-node": "^1.0.3",
    "@atcute/lexicons": "^1.2.7",
    "@atcute/tid": "^1.1.1",
    "@mantine/core": "^8.3.14",
    "@mantine/hooks": "^8.3.14",
    "@mantine/notifications": "^8.3.14",
    "@opennextjs/aws": "^3.9.15",
    "dompurify": "^3.3.1",
    "franc": "^6.2.0",
    "iso-639-3-to-1": "^1.0.0",
    "lucide-react": "^0.563.0",
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-content-loader": "^7.1.2",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "react-spinners": "^0.17.0",
    "sst": "^3.17.38",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@atcute/lex-cli": "^2.5.3",
    "@aws-sdk/client-lambda": "^3.985.0",
    "@eslint/eslintrc": "^3.3.3",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^25.2.1",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "autoprefixer": "^10.4.24",
    "eslint": "^10.0.0",
    "eslint-config-next": "^16.1.6",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "raw-loader": "^4.0.2",
    "tailwindcss": "^4.1.18",
    "tw-colors": "^3.3.2",
    "typescript": "^5.9.3"
  }
}