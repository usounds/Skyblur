{
  "name": "skyblur",
  "version": "0.3.1",
  "type": "module",
  "sideEffects": false,
  "private": true,
  "engines": {
    "node": ">=20.19.0 <21",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev -p 4500",
    "gen-lex": "lex-cli generate-main ../lexicon/uk/skyblur/*/*.json -o ./src/lexicon/UkSkyblur.ts",
    "build": "next build",
    "start": "next start",
    "dev:aws": "npx sst dev",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview --ip 192.168.1.130 --port 4500",
    "deploy:preview": "npx sst deploy --stage preview",
    "deploy": "npx sst deploy --stage production",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@atcute/bluesky": "^3.2.10",
    "@atcute/client": "^4.0.5",
    "@atcute/lexicons": "^1.2.3",
    "@atcute/oauth-browser-client": "^1.0.27",
    "@atcute/tid": "^1.0.3",
    "@mantine/core": "^8.3.9",
    "@mantine/hooks": "^8.3.9",
    "@mantine/notifications": "^8.3.9",
    "@opennextjs/aws": "^3.9.4",
    "@tabler/icons-react": "^3.34.1",
    "dompurify": "^3.2.6",
    "franc": "^6.2.0",
    "iso-639-3-to-1": "^1.0.0",
    "lucide-react": "^0.543.0",
    "next": "^15.5.9",
    "react": "^19.2.3",
    "react-content-loader": "^7.1.1",
    "react-dom": "^19.2.3",
    "react-spinners": "^0.17.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@atcute/lex-cli": "^1.1.2",
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^20.19.25",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.1",
    "eslint-config-next": "^15.5.6",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "tailwindcss": "^3.4.17",
    "tw-colors": "^3.3.2",
    "typescript": "^5.9.2"
  }
}
