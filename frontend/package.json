{
  "name": "skyblur",
  "version": "0.3.2",
  "type": "module",
  "sideEffects": false,
  "private": true,
  "engines": {
    "node": ">=20.19.0 <21",
    "pnpm": ">=9.0.0"
  },
  "pnpm": {
    "overrides": {
      "@smithy/config-resolver": ">=4.4.0"
    }
  },
  "scripts": {
    "dev": "next dev -p 4500",
    "update": "pnpm dlx npm-check-updates -u --reject \"prisma,@prisma/client\" && pnpm install",
    "gen-lex": "lex-cli generate-main ../lexicon/uk/skyblur/*/*.json -o ./src/lexicon/UkSkyblur.ts",
    "build": "next build",
    "start": "next start",
    "dev:aws": "pnpm dlx sst dev",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview --ip 192.168.1.130 --port 4500",
    "deploy:preview": "pnpm dlx sst deploy --stage preview",
    "deploy": "pnpm dlx sst deploy --stage production",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@atcute/atproto": "^3.1.10",
    "@atcute/bluesky": "^3.2.14",
    "@atcute/client": "^4.2.1",
    "@atcute/identity-resolver": "^1.2.2",
    "@atcute/lexicons": "^1.2.6",
    "@atcute/oauth-browser-client": "^2.0.3",
    "@atcute/tid": "^1.1.1",
    "@mantine/core": "^8.3.12",
    "@mantine/hooks": "^8.3.12",
    "@mantine/notifications": "^8.3.12",
    "@opennextjs/aws": "^3.9.7",
    "@tabler/icons-react": "^3.36.1",
    "dompurify": "^3.3.1",
    "franc": "^6.2.0",
    "iso-639-3-to-1": "^1.0.0",
    "lucide-react": "^0.562.0",
    "next": "^16.1.1",
    "react": "^19.2.3",
    "react-content-loader": "^7.1.1",
    "react-dom": "^19.2.3",
    "react-spinners": "^0.17.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@atcute/lex-cli": "^2.5.2",
    "@eslint/eslintrc": "^3.3.3",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.0.5",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "tailwindcss": "^4.1.18",
    "tw-colors": "^3.3.2",
    "typescript": "^5.9.3"
  }
}