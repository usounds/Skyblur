{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "skyblurapi",
  "main": "src/index.ts",
  "compatibility_date": "2025-03-10",
  "compatibility_flags": [
     "nodejs_compat"
  ],
  "vars": {
    "APPVIEW_HOST": "blursky.usounds.work"
  },
  "env": {
    "production": {
      "vars": {
        "APPVIEW_HOST": "skyblur.uk"
      },
      "routes": [
        {
          "pattern": "api.skyblur.uk",
          "custom_domain": true
        }
      ]
    }
  }
}
